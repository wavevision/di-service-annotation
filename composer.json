    "doctrine/annotations": "^1.8",
    "nette/finder": "^2.5",
    "wavevision/utils": "^2.13"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^9.4",
    "wavevision/coding-standard": "^6.0"
  },
  "extra": {
    "phpstan": {}
  }
}
